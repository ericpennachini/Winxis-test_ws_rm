        <TextBox Height="19" HorizontalAlignment="Left" Margin="117,482,0,0" Name="textBox1" VerticalAlignment="Top" Width="130" FontSize="10" IsEnabled="False" Background="LightGray"/>
        <TextBox Height="19" HorizontalAlignment="Left" Margin="267,482,0,0" Name="textBox2" VerticalAlignment="Top" Width="130" FontSize="10" IsEnabled="False" Background="LightGray"/>
        <CheckBox Content="Editar" Height="16" HorizontalAlignment="Left" Margin="233,436,0,0" Name="checkBox1" VerticalAlignment="Top" IsChecked="False" Click="checkBox1_Click" />
        <Button Content="test" Height="23" HorizontalAlignment="Left" Margin="322,120,0,0" Name="button7" VerticalAlignment="Top" Width="75" Click="button7_Click" />
        <Image Height="79" HorizontalAlignment="Left" Margin="117,12,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="280" Source="/TestForms;component/Images/logo.png" />
